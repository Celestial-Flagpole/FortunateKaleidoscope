  162    var dec;
  163    var key;
  ...
  165    var val;
  166  
  167:   for (var i = 0; i < cookies.length; i++) {
  168:     key = cookies[i];
  169      val = obj[key];
  170:     dec = signedCookie(val, secret);
  171  
  172      if (val !== dec) {

