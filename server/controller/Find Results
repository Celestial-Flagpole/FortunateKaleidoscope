 18287  
 18288  /**
 18289:  * @name $$cookieReader
 18290   * @requires $document
 18291   *
 18292   * @description
 18293:  * This is a private service for reading cookies used by $http and ngCookies
 18294   *
 18295:  * @return {Object} a key/value map of the current cookies
 18296   */
 18297: function $$CookieReader($document) {
 18298    var rawDocument = $document[0] || {};
 18299:   var lastCookies = {};
 18300:   var lastCookieString = '';
 18301  
 18302    function safeDecodeURIComponent(str) {
 .....
 18309  
 18310    return function() {
 18311:     var cookieArray, cookie, i, index, name;
 18312:     var currentCookieString = rawDocument.cookie || '';
 18313  
 18314:     if (currentCookieString !== lastCookieString) {
 18315:       lastCookieString = currentCookieString;
 18316:       cookieArray = lastCookieString.split('; ');
 18317:       lastCookies = {};
 18318  
 18319:       for (i = 0; i < cookieArray.length; i++) {
 18320:         cookie = cookieArray[i];
 18321:         index = cookie.indexOf('=');
 18322:         if (index > 0) { //ignore nameless cookies
 18323:           name = safeDecodeURIComponent(cookie.substring(0, index));
 18324:           // the first value that is seen for a cookie is the most
 18325:           // specific one.  values for the same cookie name that
 18326            // follow are for less specific paths.
 18327:           if (lastCookies[name] === undefined) {
 18328:             lastCookies[name] = safeDecodeURIComponent(cookie.substring(index + 1));
 18329            }
 18330          }
 18331        }
 18332      }
 18333:     return lastCookies;
 18334    };
 18335  }
 18336  
