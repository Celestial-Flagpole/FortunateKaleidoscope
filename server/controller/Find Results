 47792  	  return function() {
 47793: 	    var cookieArray, cookie, i, index, name;
 47794: 	    var currentCookieString = rawDocument.cookie || '';
 47795  
 47796: 	    if (currentCookieString !== lastCookieString) {
 47797: 	      lastCookieString = currentCookieString;
 47798: 	      cookieArray = lastCookieString.split('; ');
 47799: 	      lastCookies = {};
 47800  
 47801: 	      for (i = 0; i < cookieArray.length; i++) {
 47802: 	        cookie = cookieArray[i];
 47803: 	        index = cookie.indexOf('=');
 47804: 	        if (index > 0) { //ignore nameless cookies
 47805: 	          name = safeDecodeURIComponent(cookie.substring(0, index));
 47806: 	          // the first value that is seen for a cookie is the most
 47807: 	          // specific one.  values for the same cookie name that
 47808  	          // follow are for less specific paths.
 47809: 	          if (lastCookies[name] === undefined) {
 47810: 	            lastCookies[name] = safeDecodeURIComponent(cookie.substring(index + 1));
 47811  	          }
 47812  	        }
 47813  	      }
 47814  	    }
 47815: 	    return lastCookies;
 47816  	  };
