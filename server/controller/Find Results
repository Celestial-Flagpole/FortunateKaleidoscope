   40: assert.equal(cookies.length, 1, "Delete cookie fail\n" + cookies.toString());
   41: assert.equal(String(test_jar.getCookies(CookieAccessInfo("test.com","/"))), "a=1; domain=.test.com; path=/");
   42  
   43: cookie=Cookie("a=1;domain=test.com;path=/;HttpOnly");
   44: assert.ok(cookie.noscript, "HttpOnly flag parsing failed\n" + cookie.toString());
