    8  
    9: // Test Cookie
   10: var cookie = new Cookie("a=1;domain=.test.com;path=/");
   11: assert.equal(cookie.name, "a");
   12: assert.equal(cookie.value, "1");
   13: assert.equal(cookie.domain, ".test.com");
   14: assert.equal(cookie.path, "/");
   15: assert.equal(cookie.secure, false);
   16: assert.equal(cookie.expiration_date, Infinity);
   17  
   18: assert.deepEqual(cookie, new Cookie("a=1;domain=.test.com;path=/"));
   19: assert.ok(cookie.collidesWith(new Cookie("a=1;domain=.test.com;path=/")));
   20  
   21: var cookie = new Cookie("a=1;path=/", ".test.com");
   22: assert.equal(cookie.domain, ".test.com");
   23  
   24  
   25: // Test CookieJar
   26: var test_jar = CookieJar();
   27: test_jar.setCookies(
   28   "a=1;domain=.test.com;path=/"
   29   +":b=2;domain=test.com;path=/"
   30   +":c=3;domain=test.com;path=/;expires=January 1, 1970");
   31: var cookies=test_jar.getCookies(CookieAccessInfo("test.com","/"))
   32: assert.equal(cookies.length, 2, "Expires on setCookies fail\n" + cookies.toString());
   33: assert.equal(cookies.toValueString(), 'a=1;b=2', "Cannot get value string of multiple cookies");
   34  
