   76:       cookie.setMaxAge(params["max-age"]);
   77      }
   78:     cookie.secure = !!params.secure;
   79:     cookie.httpOnly = !!params.httpOnly;
   80      deleteIfExists = this.filter(function(c) {
   81:       return c.key === cookie.key && c.domain === cookie.domain && c.path === cookie.path;
   82      })[0];
