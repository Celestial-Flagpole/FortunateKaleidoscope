   40:   Cookies.prototype.select = function(identifier) {
   41:     var cookies;
   42:     cookies = this.filter(function(cookie) {
   43:       return cookie.TTL() > 0;
   44      });
   45      if (identifier.name) {
   46:       cookies = cookies.filter(function(cookie) {
   47:         return cookie.key === identifier.name;
   48        });
   49      }
   50      if (identifier.path) {
   51:       cookies = cookies.filter(function(cookie) {
   52:         return Tough.pathMatch(identifier.path, cookie.path);
   53        });
   54      }
   55      if (identifier.domain) {
   56:       cookies = cookies.filter(function(cookie) {
   57:         return Tough.domainMatch(identifier.domain, cookie.domain);
   58        });
   59      }
   60:     return cookies.sort(function(a, b) {
   61        return b.domain.length - a.domain.length;
   62:     }).sort(Tough.cookieCompare);
   63    };
