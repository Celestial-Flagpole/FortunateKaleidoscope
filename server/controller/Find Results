 1220    "Issue 1": {
 1221      topic: function() {
 1222:       var cj = new CookieJar();
 1223:       cj.setCookie('hello=world; path=/some/path/', 'http://domain/some/path/file', function(err,cookie) {
 1224:         this.callback(err,{cj:cj, cookie:cookie});
 1225        }.bind(this));
 1226      },
 1227:     "stored a cookie": function(t) {
 1228:       assert.ok(t.cookie);
 1229      },
 1230:     "cookie's path was modified to remove unnecessary slash": function(t) {
 1231:       assert.equal(t.cookie.path, '/some/path');
 1232      },
 1233      "getting it back": {
 1234        topic: function(t) {
 1235:         t.cj.getCookies('http://domain/some/path/file', function(err,cookies) {
 1236:           this.callback(err, {cj:t.cj, cookies:cookies||[]});
 1237          }.bind(this));
 1238        },
 1239:       "got one cookie": function(t) {
 1240:         assert.lengthOf(t.cookies, 1);
