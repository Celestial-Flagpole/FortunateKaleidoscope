{
  "manifest_version": 2,

  "name": "Snippets",
  "description": "This extension allows you to easily save and use snippets",
  "version": "1.0",
  "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQCM9NA1cLHYrp+htpkHWHmXKAXja/DNfyrSb2mkvfGRT/JVxbPDXJqeRXwhqwYpxgNxMxcD8skIqSy6zHWqtymzjqyFVbMg4ox7qyLC7dEMdx0TgstYjtC3KjEZNM7VM0gRlReJoQuSM/GSIGnAlUjP7Ahd1XF16JgZPICSoeFiTQIDAQAB",

  "background" : "background.html",
  "browser_action": {
    "default_icon": "scissors.png",
    "default_popup":"window.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css":[ ],
      "js": ["content.js" ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "identity",
    "cookies",
    "activeTab",
    "https://github.com/login/oauth/authorize*",
    "https://ajax.googleapis.com/",
    "http://sniphub.herokuapp.com/",
    "http://localhost:3000/auth/github",
    "https://docs.google.com/feeds/*",
  "https://www.google.com/accounts/OAuthGetRequestToken",
  "https://www.google.com/accounts/OAuthAuthorizeToken",
  "https://www.google.com/accounts/OAuthGetAccessToken"
  ]
}